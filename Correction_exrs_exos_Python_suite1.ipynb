    "compteur = 0\n",
    "total = 0\n",
    "\n",
    "def f(n):\n",
    "    global compteur, total # ne pas oublier !!\n",
    "    compteur = compteur + 1\n",
    "    total = total + n\n",
    "    print(n)\n",
    "\n",
    "for i in range(100):\n",
    "    if random() < 0.5:\n",
    "        f(i)\n",
    "\n",
    "print('compteur =', compteur)\n",
    "if compteur > 0:\n",
    "    print('moyenne =', total/compteur)"
