    "def Syracuse(n):\n",
    "    \"\"\" Affiche les valeurs successives de la suite de Syracuse \"\"\"\n",
    "    while n != 1:\n",
    "        print(n)\n",
    "        if n % 2 == 0:\n",
    "            n = n // 2\n",
    "        else:\n",
    "            n = n*3 + 1\n",
    "    print('1 - fin!')\n",
    "\n",
