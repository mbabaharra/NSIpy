    "def Syracuse_altitude(n):\n",
    "    \"\"\" Retourne l'altitude de la suite de Syracuse \"\"\"\n",
    "    altitude = 0\n",
    "    while n != 1:\n",
    "        if n > altitude:\n",
    "            altitude = n\n",
    "        if n % 2 == 0:\n",
    "            n = n // 2\n",
    "        else:\n",
    "            n = n*3 + 1\n",
    "    return altitude\n",
    "\n",
    "print(Syracuse_altitude(120))\n",
    "print(Syracuse_altitude(123))\n",
    "print(Syracuse_altitude(130))"
