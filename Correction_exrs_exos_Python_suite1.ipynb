    "def Syracuse_vol(n):\n",
    "    \"\"\" Retourne la durée de vol de la suite de Syracuse \"\"\"\n",
    "    vol = 0\n",
    "    while n != 1:\n",
    "        vol = vol + 1\n",
    "        if n % 2 == 0:\n",
    "            n = n // 2\n",
    "        else:\n",
    "            n = n*3 + 1\n",
    "    return vol\n",
    "\n",
    "print(Syracuse_vol(120))\n",
    "print(Syracuse_vol(123))\n",
    "print(Syracuse_vol(130))"
