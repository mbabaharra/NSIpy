    "print(True, 'xor', True, '=', xor(True, True))\n",
    "print(True, 'xor', False, '=', xor(True, False))\n",
    "print(False, 'xor', True, '=', xor(False, True))\n",
    "print(False, 'xor', False, '=', xor(False, False))"
